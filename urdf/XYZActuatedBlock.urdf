<?xml version="1.0" ?>

<robot name="XYZActuatedBlock">
  <link name="base_link">
    <inertial>
      <mass value="1"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.2"/>
      </geometry>
      <material name="blue">
	    <color rgba="0 0 1 1" />
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.2"/>
      </geometry>
    </collision>      
  </link>
  
  <frame link="base_link" name="body" rpy="0 0 0" xyz="0 0 0" />
  
  <force_element name="propx">
    <propellor lower_limit="0" upper_limit="10" scale_factor_thrust="1.0" scale_factor_moment="1.0">
      <parent link="base_link"/>
      <origin xyz="0 0 0"/>
      <axis xyz="1 0 0"/>
    </propellor>
  </force_element>
  
  <force_element name="propy">
    <propellor lower_limit="0" upper_limit="10" scale_factor_thrust="1.0" scale_factor_moment="1.0">
      <parent link="base_link"/>
      <origin xyz="0 0 0"/>
      <axis xyz="0 1 0"/>
    </propellor>
  </force_element>
  
  <force_element name="propz">
    <propellor lower_limit="0" upper_limit="10" scale_factor_thrust="1.0" scale_factor_moment="1.0">
      <parent link="base_link"/>
      <origin xyz="0 0 0"/>
      <axis xyz="0 0 1"/>
    </propellor>
  </force_element>
  
</robot>

